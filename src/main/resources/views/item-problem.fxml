<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<GridPane xmlns:fx="http://javafx.com/fxml/1"
          maxHeight="1.7976931348623157E308"
          maxWidth="1.7976931348623157E308"
          minHeight="-Infinity"
          minWidth="-Infinity"
          prefWidth="600.0"
          xmlns="http://javafx.com/javafx/11.0.1">
  <columnConstraints>
    <ColumnConstraints halignment="LEFT"
                       hgrow="SOMETIMES"/>
    <ColumnConstraints halignment="CENTER"
                       hgrow="ALWAYS"
                       minWidth="10.0"
                       prefWidth="100.0"/>
    <ColumnConstraints halignment="CENTER"
                       hgrow="NEVER"
                       maxWidth="1.7976931348623157E308"
                       prefWidth="100.0"/>
  </columnConstraints>
  <rowConstraints>
    <RowConstraints fillHeight="false"
                    maxHeight="1.7976931348623157E308"
                    valignment="TOP"
                    vgrow="SOMETIMES"/>
    <RowConstraints fillHeight="false"
                    maxHeight="1.7976931348623157E308"
                    vgrow="SOMETIMES"/>
  </rowConstraints>
  <children>
    <Label fx:id="id"
           alignment="TOP_LEFT"
           maxHeight="1.7976931348623157E308"
           maxWidth="1.7976931348623157E308"
           text="Label"
           GridPane.hgrow="NEVER"
           GridPane.rowSpan="2"
           GridPane.vgrow="SOMETIMES">
      <GridPane.margin>
        <Insets bottom="8.0"
                left="8.0"
                right="8.0"
                top="8.0"/>
      </GridPane.margin>
    </Label>
    <Label fx:id="title"
           alignment="TOP_LEFT"
           maxHeight="1.7976931348623157E308"
           maxWidth="1.7976931348623157E308"
           text="Lorem ipsum dolor sit amet, consectetur adipiscing elit. Nullam facilisis tellus quis ante interdum, sed sagittis ante gravida. Suspendisse tincidunt, elit at mollis rutrum, sapien urna euismod lacus, eu posuere ipsum metus ac velit. Lorem ipsum dolor sit amet, consectetur adipiscing elit. Mauris magna arcu, placerat in tristique tincidunt, rutrum eu leo."
           wrapText="true"
           GridPane.columnIndex="1"
           GridPane.halignment="LEFT"
           GridPane.hgrow="ALWAYS"
           GridPane.valignment="TOP"
           GridPane.vgrow="SOMETIMES">
      <GridPane.margin>
        <Insets bottom="8.0"
                left="8.0"
                right="8.0"
                top="8.0"/>
      </GridPane.margin>
      <font>
        <Font name="System Bold"
              size="12.0"/>
      </font>
    </Label>
    <Label fx:id="description"
           maxHeight="1.7976931348623157E308"
           maxWidth="1.7976931348623157E308"
           text="Lorem ipsum dolor sit amet, consectetur adipiscing elit. Nullam facilisis tellus quis ante interdum, sed sagittis ante gravida. Suspendisse tincidunt, elit at mollis rutrum, sapien urna euismod lacus, eu posuere ipsum metus ac velit. Lorem ipsum dolor sit amet, consectetur adipiscing elit. Mauris magna arcu, placerat in tristique tincidunt, rutrum eu leo."
           wrapText="true"
           GridPane.columnIndex="1"
           GridPane.halignment="LEFT"
           GridPane.hgrow="ALWAYS"
           GridPane.rowIndex="1"
           GridPane.valignment="TOP"
           GridPane.vgrow="SOMETIMES">
      <GridPane.margin>
        <Insets bottom="8.0"
                left="8.0"
                right="8.0"
                top="8.0"/>
      </GridPane.margin>
    </Label>
    <ProgressBar fx:id="progress"
                 maxHeight="1.7976931348623157E308"
                 maxWidth="1.7976931348623157E308"
                 progress="0.0"
                 GridPane.columnIndex="2"
                 GridPane.halignment="CENTER"
                 GridPane.hgrow="SOMETIMES"
                 GridPane.valignment="BOTTOM"
                 GridPane.vgrow="SOMETIMES">
      <GridPane.margin>
        <Insets bottom="8.0"
                left="8.0"
                right="8.0"
                top="8.0"/>
      </GridPane.margin>
    </ProgressBar>
    <Label fx:id="status"
           alignment="CENTER"
           maxHeight="1.7976931348623157E308"
           maxWidth="1.7976931348623157E308"
           text="No started yet"
           GridPane.columnIndex="2"
           GridPane.halignment="CENTER"
           GridPane.hgrow="SOMETIMES"
           GridPane.rowIndex="1"
           GridPane.valignment="TOP"
           GridPane.vgrow="SOMETIMES">
      <GridPane.margin>
        <Insets bottom="8.0"
                left="8.0"
                right="8.0"
                top="8.0"/>
      </GridPane.margin>
    </Label>
  </children>
</GridPane>
